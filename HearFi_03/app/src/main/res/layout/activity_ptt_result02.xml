<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.PTT.PttResult02Activity"
    android:background="@color/white">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal"

                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="-1dp">
                <ImageButton
                    android:id="@+id/imgBtnBack"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="start"
                    android:layout_marginEnd="20dp"
                    android:layout_weight="0"
                    android:background="@color/white"
                    android:padding="13dp"
                    android:scaleType="fitCenter"
                    app:srcCompat="@drawable/backbtn" />
                <LinearLayout
                    android:layout_weight="0.5"
                    android:layout_width="match_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="match_parent"
                        android:textAlignment="center"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/pretendardextrabold"
                        android:textColor="@color/blue"
                        android:textSize="25dp"
                        android:text="HearFi"></TextView>
                </LinearLayout>

                <ImageButton
                    android:id="@+id/imgBtnHome"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="end"
                    android:layout_marginStart="20dp"
                    android:layout_weight="0"
                    android:background="@color/white"
                    android:padding="13dp"
                    android:scaleType="fitCenter"
                    app:srcCompat="@drawable/homebtn" />
            </LinearLayout>
            <TextView
                android:id="@+id/chartTitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="oo님의 오디오그램"
                android:textAlignment="center"
                android:fontFamily="@font/pretendardbold"
                android:textSize="21dp"
                android:layout_marginTop="10dp"></TextView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/custom_edittext"
                android:layout_gravity="center"
                android:padding="10dp"
                android:paddingRight="5dp"
                android:orientation="horizontal">

                <com.github.mikephil.charting.charts.LineChart
                    android:id="@+id/LineChart"
                    android:layout_width="match_parent"
                    android:layout_height="500dp">
                </com.github.mikephil.charting.charts.LineChart>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/custom_edittext"
                android:orientation="vertical"
                android:layout_gravity="center">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="8dp"

                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="(단위 : dBHL)"
                        android:layout_marginStart="16dp"
                        android:layout_marginEnd="16dp"
                        android:textAlignment="viewEnd"
                        android:fontFamily="@font/pretendardbold"
                        android:textColor="@color/gray"></TextView>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="16dp"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="0.15"
                        android:orientation="vertical"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/hr1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="정상청력"
                            android:fontFamily="@font/pretendardregular"
                            android:textAlignment="center"
                            android:textSize="12dp"
                            android:textColor="#a6a6a6"></TextView>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_weight="0.15"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">

                            <View
                                android:layout_weight="0.1"
                                android:layout_width="2dp"
                                android:layout_height="20dp"
                                android:background="@color/gray"></View>
                            <View
                                android:id="@+id/v1"
                                android:layout_weight="0.85"
                                android:layout_gravity="center"
                                android:layout_width="45dp"
                                android:layout_height="1dp"
                                android:background="@color/gray"></View>
                            <View
                                android:layout_weight="0.05"
                                android:layout_width="1dp"
                                android:layout_height="20dp"
                                android:background="@color/gray"></View>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="0.15"
                        android:orientation="vertical"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/hr2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="경도난청"
                            android:fontFamily="@font/pretendardregular"
                            android:textAlignment="center"
                            android:textSize="12dp"
                            android:textColor="#a6a6a6"></TextView>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_weight="0.15"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                            <View
                                android:layout_weight="0.1"
                                android:layout_width="1dp"
                                android:layout_height="20dp"
                                android:background="@color/gray"></View>
                            <View
                                android:id="@+id/v2"
                                android:layout_weight="0.85"
                                android:layout_gravity="center"
                                android:layout_width="45dp"
                                android:layout_height="1dp"
                                android:background="@color/gray"></View>
                            <View
                                android:layout_weight="0.05"
                                android:layout_width="1dp"
                                android:layout_height="20dp"
                                android:background="@color/gray"></View>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="0.15"
                        android:orientation="vertical"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/hr3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="중도난청"
                            android:fontFamily="@font/pretendardregular"
                            android:textAlignment="center"
                            android:textSize="12dp"
                            android:textColor="#a6a6a6"></TextView>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_weight="0.15"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                            <View
                                android:layout_weight="0.1"
                                android:layout_width="1dp"
                                android:layout_height="20dp"
                                android:background="@color/gray"></View>
                            <View
                                android:id="@+id/v3"
                                android:layout_weight="0.85"
                                android:layout_gravity="center"
                                android:layout_width="45dp"
                                android:layout_height="1dp"
                                android:background="@color/gray"></View>
                            <View
                                android:layout_weight="0.05"
                                android:layout_width="1dp"
                                android:layout_height="20dp"
                                android:background="@color/gray"></View>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="0.15"
                        android:orientation="vertical"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/hr4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="중고도난청"
                            android:fontFamily="@font/pretendardregular"
                            android:textAlignment="center"
                            android:textSize="12dp"
                            android:textColor="#a6a6a6"></TextView>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_weight="0.15"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                            <View
                                android:layout_weight="0.1"
                                android:layout_width="1dp"
                                android:layout_height="20dp"
                                android:background="@color/gray"></View>
                            <View
                                android:id="@+id/v4"
                                android:layout_weight="0.85"
                                android:layout_gravity="center"
                                android:layout_width="45dp"
                                android:layout_height="1dp"
                                android:background="@color/gray"></View>
                            <View
                                android:layout_weight="0.05"
                                android:layout_width="1dp"
                                android:layout_height="20dp"
                                android:background="@color/gray"></View>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="0.15"
                        android:orientation="vertical"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/hr5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="고도난청"
                            android:textAlignment="center"
                            android:fontFamily="@font/pretendardregular"
                            android:textSize="12dp"
                            android:textColor="#a6a6a6"></TextView>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_weight="0.15"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                            <View
                                android:layout_weight="0.1"
                                android:layout_width="1dp"
                                android:layout_height="20dp"
                                android:background="@color/gray"></View>
                            <View
                                android:id="@+id/v5"
                                android:layout_weight="0.85"
                                android:layout_gravity="center"
                                android:layout_width="45dp"
                                android:layout_height="1dp"
                                android:background="@color/gray"></View>
                            <View
                                android:layout_weight="0.05"
                                android:layout_width="1dp"
                                android:layout_height="20dp"
                                android:background="@color/gray"></View>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="0.15"
                        android:orientation="vertical"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/hr6"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="심도난청"
                            android:fontFamily="@font/pretendardregular"
                            android:textAlignment="center"
                            android:textSize="12dp"
                            android:textColor="#a6a6a6"></TextView>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_weight="0.15"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                            <View
                                android:layout_weight="0.1"
                                android:layout_width="1dp"
                                android:layout_height="20dp"
                                android:background="@color/gray"></View>
                            <View
                                android:id="@+id/v6"
                                android:layout_weight="0.85"
                                android:layout_gravity="center"
                                android:layout_width="45dp"
                                android:layout_height="1dp"
                                android:background="@color/gray"></View>
                            <View
                                android:layout_weight="0.1"
                                android:layout_width="2dp"
                                android:layout_height="20dp"
                                android:background="@color/gray"></View>
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="14dp"
                    android:layout_marginEnd="27dp"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.3333"
                            android:text="0"
                            android:textColor="@color/gray"></TextView>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.3333"
                            android:text="20"
                            android:textColor="@color/gray"></TextView>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.3333"
                            android:text="40"
                            android:textColor="@color/gray"></TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.3333"
                            android:text="55"
                            android:textColor="@color/gray"></TextView>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.3333"
                            android:text="70"
                            android:textColor="@color/gray"></TextView>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.3333"
                            android:text="90"
                            android:textColor="@color/gray"></TextView>
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/custom_blue_radius_background"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/textAccountName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:textColor="@color/white"
                    android:textSize="21dp"
                    android:fontFamily="@font/pretendardregular"
                    android:textAlignment="center"
                    android:text="김연우 님의 테스트 결과는"></TextView>
                <TextView
                    android:id="@+id/textRightResult"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:textColor="@color/white"
                    android:textSize="24dp"
                    android:fontFamily="@font/pretendardbold"
                    android:textAlignment="center"
                    android:text="중도난청(41~55dBHL)"></TextView>
                <TextView
                    android:id="@+id/textLeftResult"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:textColor="@color/white"
                    android:textSize="24dp"
                    android:fontFamily="@font/pretendardbold"
                    android:textAlignment="center"
                    android:text="중도난청(41~55dBHL)"></TextView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:textSize="21dp"

                    android:fontFamily="@font/pretendardregular"
                    android:textAlignment="center"
                    android:text="으로 예상됩니다."></TextView>
                <View
                    android:layout_width="match_parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_height="2dp"
                    android:background="@color/white">
                </View>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="16dp"
                    android:textColor="@color/white"
                    android:textSize="17dp"

                    android:fontFamily="@font/pretendardbold"
                    android:textAlignment="center"
                    android:text="향후, 가까운 이비인후과 병원을 방문하여\n정확한 난청 진단을 받아보시기 바랍니다."></TextView>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="16dp">
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/hlinfoBtn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/blue_button"
                    android:text="난청 정보 알아보기"
                    android:textColor="@color/white"
                    android:textSize="17dp"
                    android:textStyle="bold">
                </androidx.appcompat.widget.AppCompatButton>
            </LinearLayout>




        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>